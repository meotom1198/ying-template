    background: var(--main-bg-color);
    border-radius: 5px;
    background-position: center center;
    background-size: cover;
    position: relative;
    overflow: hidden;
}
.next-1, .prev-1 {
    display: inline-block;
    width: 100%;
}
.next-1 span {
    float: right;
    font-size: .9rem;
    padding-bottom: .5rem;
}
.prev-1 span {
    float: left;
    font-size: .9rem;
    padding-bottom: .5rem;
}
.prev_box, .next_box {
    display: inline-block;
    color: #fff;
    font-size: .9rem;
    padding: 1rem;
    box-sizing: border-box;
    background: rgba(0, 0, 0, 0.2);
}
.post-copyright {
    font-size: .9rem;
    padding: 1rem .5rem;
    margin-top: 1rem;
    font-family: 'Dosis', serif;
    background: var(--bg1-color);
    border-left: 4px solid #50c0f3;
    position: relative;
}
.post-copyright:before {
    position: absolute;
    top: 2px;
    right: 12px;
    color: #49b1f5;
    content: '🅲';
    font-size: 1.3em;
}

.tepasspost {
    font-size: .8rem;
}
.post-copyright-info {
    padding: 4px;
}
.post-copyright-info span {
    display: inline;
    word-break: break-all;
    line-height: 1.5;
}
.cometa {
    color: var(--theme-color);
}
@media(max-width:500px) {
    .comment-box {
        padding: 0 .5rem .5rem;
        margin-top: .5rem;
    }
    .commet-box {
        padding-bottom: .5rem;
        max-width: 100%;
    }
    .comment-children .cot-body {
        padding-left: 25px;
    }
    pre.line-numbers {
        padding-left: 3em;
    }
    .gallery-page {
        padding: 1rem .5rem;
    }
}

